
if not getgenv().script_key or getgenv().script_key == "" then
    game.Players.LocalPlayer:Kick("No key found!")
    return
end


local success, result = pcall(function()
    return loadstring(game:HttpGet('https://raw.githubusercontent.com/Kokiuvu123/GPO/main/verify.lua'))()
end)

if not success then
    game.Players.LocalPlayer:Kick("Failed to load verification script!")
    return
end


local is_valid = verifyKey(getgenv().script_key)

if not is_valid then
    game.Players.LocalPlayer:Kick("Invalid key!")
    return
end

print("Key is valid, script continues!")
